#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        string str="";
        string str1="";
        cin>>str;
        for(int i=0;i<n-1;i+=2)
        {
            if(str[i]=='0'&&str[i+1]=='0')
            cout<<'A';
            else if(str[i]=='0'&&str[i+1]=='1')
            cout<<'T';
            else if(str[i]=='1'&&str[i+1]=='0')
            cout<<'C';
            if(str[i]=='1'&&str[i+1]=='1')
            cout<<'G';
        }
        cout<<endl;
    }
	// your code goes here
	return 0;
}
