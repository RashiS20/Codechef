#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        n--;
        int ans=(n/5)*2;
        n%=5;
        if(n==4)
        ans+=2;
        else if(n!=0)
        ans++;
        cout<<ans<<endl;
    }
	// your code goes here
	return 0;
}
