#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int a,b,c,d;
        cin>>a>>b>>c>>d;
        int max1=max(a,b);
        int max2=max(c,d);
        int max_f=max(max1,max2);
        cout<<max_f<<endl;
    }
	// your code goes here
	return 0;
}
