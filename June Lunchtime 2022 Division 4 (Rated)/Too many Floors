#include <bits/stdc++.h>
using namespace std;

int add1(int a,int b)
{
    return a+b;
}
int sub1(int a,int b)
{
    return a-b;
}
int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int x,y;
        cin>>x>>y;
        int ans1=ceil(x/10.0);
        int ans2=ceil(y/10.0);
        cout<<abs(ans1-ans2)<<endl;
    }
	// your code goes here
	return 0;
}
